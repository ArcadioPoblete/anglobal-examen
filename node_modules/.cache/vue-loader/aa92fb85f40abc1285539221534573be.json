) total02 += e\n    \n\n      this.$store.state.monto01 = total01\n      this.$store.state.monto02 = total02\n\n\n        this.charData = {\n          labels: ['CUENTA: ****6789', 'CUENTA: ****4321'],\n          datasets: [\n            {\n              label: 'Data One',\n              backgroundColor: '#f87979',\n              data: [total01,total02],\n              backgroundColor: ['#4DB6AC','#00897B']\n            }\n          ]\n        }\n  },\n  methods: {\n\n  },\n  components:{\n    Header,\n    Chart,\n    card01,\n    card02\n  }\n}\n</script>\n"]}]}