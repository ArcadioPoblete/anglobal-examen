\"12\" class=\"pb-0\">\n                      <v-select\n                        :items=\"selectAccount\"\n                        label=\"Accounts\"\n                      ></v-select>\n                    </v-col> \n                    <v-col cols=\"12\" class=\"pt-0 pb-0\">\n                       <v-text-field \n                        v-model=\"desAccount\" \n                        label=\"Destination account\"\n                      ></v-text-field>\n                    </v-col>\n                    <v-col cols=\"12\" class=\"pt-0 pb-0\">\n                       <v-text-field \n                        v-model=\"amount\" \n                        label=\"Amount\"\n                      ></v-text-field>\n                    </v-col>\n                    <v-col cols=\"12\" class=\"pt-0\">\n                      <v-btn \n                        text \n                        small \n                        @click=\"$refs.formTransfer.reset()\"\n                        class=\"pa-0\"\n                      >Limpiar formulario</v-btn>\n                    </v-col> \n                  </v-row>\n                </v-form>\n              </v-card-text>\n              <v-divider></v-divider>\n              <v-card-actions class=\"pa-0\">\n              <v-spacer></v-spacer>\n              <v-btn\n                  color=\"green darken-3\"\n                  tile\n                  dark\n                  depressed\n                  class=\"btnTrans\"\n              >Transfer</v-btn>\n              <v-btn\n                  color=\"blue-grey lighten-4\"\n                  tile\n                  depressed\n                  class=\"btnTrans ml-0\"\n              >Cancel</v-btn>\n              </v-card-actions>\n            </v-card>\n        </v-col>\n        <v-col cols=\"6\" md=\"6\" lg=\"8\" xl=\"4\" class=\"d-flex justify-center\">\n          <chart02 :chardata=\"chardata\" :options=\"options\" />\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col cols=\"6\" md=\"6\" lg=\"6\" xl=\"6\" v-for=\"(itemAccounts, index) in $store.state.accounts\" :key=\"index\">\n           <v-card>\n             <v-card-title>\n               {{ itemAccounts.account }}\n             </v-card-title>\n             <v-card-text>\n               <v-simple-table>\n                <template v-slot:default>\n                    <thead>\n                    <tr>\n                        <th class=\"text-center\">Origin Account</th>\n                        <th class=\"text-center\">Destination account</th>\n                        <th class=\"text-center\">Transfer date</th>\n                        <th class=\"text-center\">Amount</th>\n                    </tr>\n                    </thead>\n                    <tbody>\n                    <tr v-for=\"(item, index) in $store.state.transactions \" :key=\"index\" \n                        v-show=\"item.fromAccount \">\n                        <td class=\"text-center\">{{ item.fromAccount}}</td>\n                        <td class=\"text-center\">{{ item.toAccount }}</td>\n                        <td class=\"text-center\">{{ item.sentAt.substr(0,10)}}</td>\n                        <td class=\"text-center\">{{item.amount.currency}} {{item.amount.value}}</td>\n                    </tr>\n                    </tbody>\n                </template>\n              </v-simple-table>\n             </v-card-text>\n           </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n    </div>\n</template>\n\n<script>\nimport Header from '../components/Header'\nimport chart02 from '../components/Chart02'\n\nexport default {\n  data: () => ({\n    selectAccount: [],\n    groupAcc: [],\n    desAccount: '',\n    amount: '',\n    chardata: {\n      labels: ['Enero', 'Febrero'],\n      datasets: [\n        {\n          label: 'Data One',\n          backgroundColor: '#f87979',\n          data: [40, 20]\n        }\n      ]\n     },\n    options: {\n      responsive: true,\n      maintainAspectRatio: true\n    }\n  }),\n  created (){\n    //Grupo de cuentas\n    let cuentas = this.$store.state.accounts\n    let transactions = $store.state.transactions\n\n    cuentas.forEach( item => {\n        this.selectAccount.push('*****' + item.account.substr(0,5)+ ' - ' + item.currency + item.total)\n    });\n\n    //Grupo de cuentas 01\n    cuentas.forEach( item => {\n        this.groupAccounts.push(item)\n    });\n  },\n  components: {\n      Header,\n      chart02\n  }\n}\n</script>\n"]}]}